{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "HR": "Debug"
    }
  },
  "Serilog": {
    "FileLogging": {
      "PathFormat": "C:/Development/HR.AnsConnector-Logs/ts-{Date}.log",
      "Json": false,
      "FileSizeLimitBytes": 1073741824,
      "RetainedFileCountLimit": 365,
      "OutputTemplate": "{Timestamp:o} [{Level:u3}] {SourceContext} ({EventId:x8}): {Message:l}{NewLine}{Exception}",
      "LogLevel:Default": "Information"
    }
  },
  "ConnectionStrings": {
    "AnsDbContext": ""
  },
  "BatchSettings": {
    "BatchSize": 20,
    "TimeDelayBetweenRuns": "00:00:10"
  },
  "ApiSettings": {
    "BaseUri": "https://ans.app/api/v2/",
    "BearerToken": "",
    "TenantId": 0
  }
}
